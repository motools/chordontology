<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	xmlns:ch="http://purl.org/ontology/chord/">

	<!-- ONTOLOGY DEFINITION -->

	<owl:Ontology rdf:about="">
		<dc:title>The Chord Ontology</dc:title>
		<owl:versionInfo>Revision: 0.1</owl:versionInfo>
		<dc:description>FIXME
		</dc:description>
		<foaf:maker rdf:resource="http://chrissutton.org/me"/>
		<dc:date>2007-09-11</dc:date>
	</owl:Ontology>


	<!-- CLASSES -->
	
	<rdfs:Class rdf:about="http://purl.org/ontology/chord/Chord" rdfs:label="Chord">
		<rdfs:comment>Two or more notes played together.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
	</rdfs:Class>
	
	<rdfs:Class rdf:about="http://purl.org/ontology/chord/Note" rdfs:label="Note">
		<rdfs:comment>FIXME</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
	</rdfs:Class>
	
	<rdfs:Class rdf:about="http://purl.org/ontology/chord/Natural" rdfs:label="Natural">
		<rdfs:comment>One of the seven natural notes of the Western music system.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://purl.org/ontology/chord/Note" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
	</rdfs:Class>
	
	<rdfs:Class rdf:about="http://purl.org/ontology/chord/ComponentNote" rdfs:label="ComponentNote">
		<rdfs:comment>A note which is part of a chord.</rdfs:comment>
		<rdfs:subClassOf rdf:resource="http://purl.org/ontology/chord/Note" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
	</rdfs:Class>	
	
	<rdfs:Class rdf:about="http://purl.org/ontology/chord/Modifier" rdfs:label="Modifier">
		<rdfs:comment>A modifier applied to a note to change its pitch.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
	</rdfs:Class>
	
	<rdfs:Class rdf:about="http://purl.org/ontology/chord/Degree" rdfs:label="Degree">
		<rdfs:comment>A degree of the root scale, made up of an interval and optional modifier.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class" />
	</rdfs:Class>


	
	<!-- Properties -->
	
	<!-- Chord Properties -->
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/baseChord" rdfs:label="baseChord">
		<rdfs:comment>The chord on which this one is based. 
			For example, a C7 chord might have ch:Cmaj as its base chord.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Chord" />
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/Chord" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/root" rdfs:label="root">
		<rdfs:comment>The root note of the chord.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Chord" />
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/Note" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/bass" rdfs:label="bass">
		<rdfs:comment>The bass note of the chord (indicates the inversion).</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Chord" />
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/Degree" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/degree" rdfs:label="degree">
		<rdfs:comment>A degree of the scale which is part of the chord.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Chord" />
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/Degree" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>

	<rdf:Property rdf:about="http://purl.org/ontology/chord/without_degree" rdfs:label="without_degree">
		<rdfs:comment>A degree of the scale expected in the chord but not actually present here.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Chord" />
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/Degree" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/component" rdfs:label="component">
		<rdfs:comment>A component of the chord.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Chord" />
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/ComponentNote" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>
	
	
	<!-- Other Properties -->
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/natural" rdfs:label="natural">
		<rdfs:comment>The natural from which this note is derived.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Note" />
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/Natural" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>	
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/modifier" rdfs:label="modifier">
		<rdfs:comment>A modification to a note's pitch.</rdfs:comment>
		<rdfs:domain>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<owl:Class rdf:about="http://purl.org/ontology/chord/Note"/>
					<owl:Class rdf:about="http://purl.org/ontology/chord/Degree"/>
				</owl:unionOf>
			</owl:Class>
		</rdfs:domain>
		<rdfs:range rdf:resource="http://purl.org/ontology/chord/Modifier" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>
	
	<rdf:Property rdf:about="http://purl.org/ontology/chord/scale_interval" rdfs:label="scale_interval">
		<rdfs:comment>A major diatonic interval from the root of a chord.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/Degree" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>

	<rdf:Property rdf:about="http://purl.org/ontology/chord/semitone_interval" rdfs:label="semitone_interval">
		<rdfs:comment>An interval measured in semitones.</rdfs:comment>
		<rdfs:domain rdf:resource="http://purl.org/ontology/chord/ComponentNote" />
		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer" />
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</rdf:Property>
	
	<!-- Instances -->
	
	<!-- Chord types -->
	<ch:Chord rdf:about="http://purl.org/ontology/chord/noChord" rdfs:label="noChord">
		<rdfs:comment>Indicates that no chord is played.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Chord>

	<ch:Chord rdf:about="http://purl.org/ontology/chord/maj" rdfs:label="maj">
		<rdfs:comment>A major chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>3</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/min" rdfs:label="min">
		<rdfs:comment>A minor chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>	

	<ch:Chord rdf:about="http://purl.org/ontology/chord/dim" rdfs:label="dim">
		<rdfs:comment>A diminished chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>5</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
	</ch:Chord>	

	<ch:Chord rdf:about="http://purl.org/ontology/chord/aug" rdfs:label="aug">
		<rdfs:comment>An augmented chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>3</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>5</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/sharp"/>
		</ch:Degree></ch:degree>
	</ch:Chord>

	<ch:Chord rdf:about="http://purl.org/ontology/chord/maj7" rdfs:label="maj7">
		<rdfs:comment>A major seventh chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>3</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>7</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/7" rdfs:label="7">
		<rdfs:comment>A seventh chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>3</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>7</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
	</ch:Chord>

	<ch:Chord rdf:about="http://purl.org/ontology/chord/min7" rdfs:label="min7">
		<rdfs:comment>A minor seventh chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>7</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
	</ch:Chord>
		
	<ch:Chord rdf:about="http://purl.org/ontology/chord/dim7" rdfs:label="dim7">
		<rdfs:comment>A diminished seventh chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>5</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>7</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/doubleflat"/>
		</ch:Degree></ch:degree>
	</ch:Chord>

	<ch:Chord rdf:about="http://purl.org/ontology/chord/hdim7" rdfs:label="hdim7">
		<rdfs:comment>A half-diminished seventh chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>5</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>7</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/minmaj7" rdfs:label="minmaj7">
		<rdfs:comment>A minor (major 7th) chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>7</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/maj6" rdfs:label="maj6">
		<rdfs:comment>A major sixth chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>3</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>6</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/min7" rdfs:label="min7">
		<rdfs:comment>A minor sixth chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>6</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/9" rdfs:label="9">
		<rdfs:comment>A ninth chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>3</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>7</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>9</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/maj9" rdfs:label="maj9">
		<rdfs:comment>A major ninth chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>3</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>7</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>9</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>	
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/min9" rdfs:label="min9">
		<rdfs:comment>A minor ninth chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree>
			<ch:scale_interval>3</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree>
			<ch:scale_interval>7</ch:scale_interval>
			<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		</ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>9</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<ch:Chord rdf:about="http://purl.org/ontology/chord/sus4" rdfs:label="sus4">
		<rdfs:comment>A suspended fourth chord.</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
		<ch:degree><ch:Degree><ch:scale_interval>4</ch:scale_interval></ch:Degree></ch:degree>
		<ch:degree><ch:Degree><ch:scale_interval>5</ch:scale_interval></ch:Degree></ch:degree>
	</ch:Chord>
	
	<!-- Naturals -->
	<ch:Natural rdf:about="http://purl.org/ontology/chord/note/A" rdfs:label="A">
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Natural>
	<ch:Natural rdf:about="http://purl.org/ontology/chord/note/B" rdfs:label="B">
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Natural>
	<ch:Natural rdf:about="http://purl.org/ontology/chord/note/C" rdfs:label="C">
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Natural>
	<ch:Natural rdf:about="http://purl.org/ontology/chord/note/D" rdfs:label="D">
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Natural>
	<ch:Natural rdf:about="http://purl.org/ontology/chord/note/E" rdfs:label="E">
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Natural>
	<ch:Natural rdf:about="http://purl.org/ontology/chord/note/F" rdfs:label="F">
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Natural>
	<ch:Natural rdf:about="http://purl.org/ontology/chord/note/G" rdfs:label="G">
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Natural>
	
	<!-- Other useful notes -->
	<ch:Note rdf:about="http://purl.org/ontology/chord/note/Aflat" rdfs:label="Ab">
		<ch:natural rdf:resource="http://purl.org/ontology/chord/note/A"/>
		<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Note>
	<ch:Note rdf:about="http://purl.org/ontology/chord/note/Bflat" rdfs:label="Bb">
		<ch:natural rdf:resource="http://purl.org/ontology/chord/note/B"/>
		<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Note>
	<ch:Note rdf:about="http://purl.org/ontology/chord/note/Cflat" rdfs:label="Cb">
		<ch:natural rdf:resource="http://purl.org/ontology/chord/note/C"/>
		<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
	</ch:Note>
	<ch:Note rdf:about="http://purl.org/ontology/chord/note/Dflat" rdfs:label="Db">
		<ch:natural rdf:resource="http://purl.org/ontology/chord/note/D"/>
		<ch:modifier rdf:resource="http://purl.org/ontology/chord/flat"/>
		<rdfs:isDefinedBy rdf:resource="http://purl.org/ontology/chord/" />
